{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "SimpleGenericAgChartOptions",
    "type": "object",
    "properties": {
      "chart_type": { "type": "string", "description": "line, bar, area, scatter, pie" },
      "x_key": { "type": "string" },
      "y_keys": { "type": "array", "items": { "type": "string" } },
      "series": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "type": { "type": "string" },
            "xKey": { "type": "string" },
            "yKey": { "type": "string" }
          },
          "required": ["type", "xKey", "yKey"]
        }
      }
    },
    "required": ["chart_type", "x_key", "series"]
  }
  